package terminverwaltung;


import java.util.ArrayList;
/**
 *
 * @author stud
 */
public class Raumverwaltung {

    public Raumverwaltung() {
        
    }

  public void createRaum(String name, String location, String plaetze){
    Raum newraum = new Raum();
    newraum.setName(name);
    newraum.setLocation(location);
    newraum.setPlaetze(plaetze);
    Raumliste.add(newraum);
}
    
    private ArrayList<Raum>Raumliste;

}